{
  "name": "tagebuch",
  "scripts": {
    "start": "node static.js",
    "build": "npm run --silent clean && . ./loadenv.sh && webpack --mode production",
    "clean": "find dist/* -not -name 'index.html' -not -name '.gitignore' | xargs rm -rf",
    "deploy": "./deploy.sh",
    "lint": "eslint 'src/**/*.{js,vue}'",
    "fmt": "npm run --silent lint -- --fix",
    "dev": ". ./loadenv.sh && webpack-dev-server --mode development",
    "setup": "rm -rf node_modules functions/node_modules && yarn && yarn --cwd functions",
    "heroku-postbuild": ".heroku/setup.sh"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-config-vue": "2.0.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "eslint-plugin-vue": "5.0.0",
    "firebase-tools": "6.1.2",
    "vue-template-compiler": "2.5.18",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "@ajusa/lit": "1.1.0",
    "@firebase/app": "0.3.5",
    "@firebase/auth": "0.5.3",
    "@firebase/firestore": "0.5.6",
    "@firebase/storage": "0.2.4",
    "axios": "0.18.0",
    "dotenv": "5.0.1",
    "express": "4.16.4",
    "http-proxy-middleware": "0.19.1",
    "marked": "0.6.2",
    "mini-css-extract-plugin": "0.4.5",
    "monaco-editor": "0.13.1",
    "raven-js": "3.27.0",
    "spinkit": "1.2.5",
    "uuid": "3.3.2",
    "vue": "2.5.18",
    "vue-async-computed": "3.5.0",
    "vue-loader": "15.4.2",
    "vue-router": "3.0.2"
  }
}
