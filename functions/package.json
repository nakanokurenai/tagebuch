    "deploy": "npx --prefix ../ firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "6.5.1",
    "firebase-functions": "1.0.3"
  },
  "devDependencies": {
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  },
  "private": true
}
